<p id="notice"><%= notice %></p>

<h1>Oscare Male Winners</h1>

<table>
  <thead>
    <tr>
      <th>Winnerid</th>
      <th>Year</th>
      <th>Age</th>
      <th>Name</th>
      <th>Movie</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @oscare_male_winners.each do |oscare_male_winner| %>
      <tr>
        <td><%= oscare_male_winner.winnerId %></td>
        <td><%= oscare_male_winner.year %></td>
        <td><%= oscare_male_winner.age %></td>
        <td><%= oscare_male_winner.name %></td>
        <td><%= oscare_male_winner.movie %></td>
        <td><%= link_to 'Show', oscare_male_winner %></td>
        <td><%= link_to 'Edit', edit_oscare_male_winner_path(oscare_male_winner) %></td>
        <td><%= link_to 'Destroy', oscare_male_winner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
